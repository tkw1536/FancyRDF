{
    "name": "tkw1536/fancy_sparql",
    "description": "Fancy SPARQL library",
    "type": "library",
    "version": "1.0.0-alpha1",
    "authors": [
        {
            "name": "Tom Wiesing",
            "homepage": "https://tkw01536.de/"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-pcre": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.0",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "doctrine/coding-standard": "^13.0"
    },
    "autoload": {
        "psr-4": {
            "FancySparql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FancySparql\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
