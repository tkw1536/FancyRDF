{
    "name": "tkw1536/fancy_sparql",
    "description": "Fancy SPARQL library",
    "type": "library",
    "version": "1.0.0-alpha1",
    "authors": [
        {
            "name": "Tom Wiesing",
            "homepage": "https://tkw01536.de/"
        }
    ],
    "scripts": {
        "dev": [
            "@dev-cspell",
            "@dev-lint",
            "@dev-test"
        ],
        "dev-lint": [
            "@dev-composer-dependency-analyser",
            "@dev-parallel-lint",
            "@dev-phpcs",
            "@dev-phpstan"
        ],
        "dev-parallel-lint": [
            "vendor/bin/parallel-lint src tests"
        ],
        "dev-phpcs": [
            "vendor/bin/phpcs"
        ],
        "dev-phpstan": [
            "vendor/bin/phpstan analyse --memory-limit=1G -v"
        ],
        "dev-composer-dependency-analyser": [
            "vendor/bin/composer-dependency-analyser --verbose"
        ],
        "dev-fmt": [
            "@dev-phpcbf"
        ],
        "dev-phpcbf": [
            "vendor/bin/phpcbf"
        ],
        "dev-test": [
            "@dev-phpunit-assertions",
            "@dev-phpunit-noassertions"
        ],
        "dev-phpunit-assertions": [
            "php -dzend.assertions=1 vendor/bin/phpunit --display-deprecations --display-warnings"
        ],
        "dev-phpunit-noassertions": [
            "php -dzend.assertions=0 vendor/bin/phpunit --display-deprecations --display-warnings"
        ],
        "dev-cspell": [
            "cspell ."
        ]
    },
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-xmlreader": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^13.0",
        "squizlabs/php_codesniffer": "^3.0",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "doctrine/coding-standard": "^13.0",
        "shipmonk/composer-dependency-analyser": "^1.8",
        "ext-sockets": "*",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-phpunit": "^2.0",
        "sirbrillig/phpcs-variable-analysis": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "FancyRDF\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FancyRDF\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}